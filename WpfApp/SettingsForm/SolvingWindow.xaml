        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp.SettingsForm"
        mc:Ignorable="d"
        Title="SolvingWindow" Height="400" Width="400" Loaded="Window_Loaded" ResizeMode="NoResize" Icon="/WpfApp;component/Resources/GameIcon.ico" WindowStartupLocation="CenterOwner" AllowsTransparency="True" WindowStyle="None" Visibility="Visible" Background="{x:Null}">
    <Grid>


        <TextBlock x:Name="textBlock" Margin="69.5,150,30.5,150" TextWrapping="Wrap" Text="Solving..." FontSize="72" Foreground="Blue" TextDecorations="{x:Null}" UseLayoutRounding="False"/>
    </Grid>
</Window>
